void setup (){
  size (800,800);
  background (255);
  noFill ();
  stroke (100);
  strokeWeight (1.5);
  
  translate (width/2, height/2); // (0,0) is at center
  
  drawEye (400,200);
  
}

void drawEye(int w, int h){
  int x = 2; // I think I can use this for the distance in between each layer?
  if (w < 100 || h < 50) return;
  
  drawEye (h, h * x);
  rotate (HALF_PI);
  ellipse (0,0,w,h);
}
