void setup (){
 size (800,800);
  background (255);
  noFill ();
  stroke (100);
  strokeWeight (1.5);
  
  translate (width/2, height/2); 
  
  drawEye (150,100);
  
}

void drawEye(int w, int h){
  int x = 11; 
  if (w < 10 || h < 5) return;
  
  drawEye (h, h - x);
  rotate (HALF_PI);
  ellipse (0,0,w,h);
}
